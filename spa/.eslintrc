{
  "plugins": [
    "compat",
    "flowtype"
  ],
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended"
  ],
  "rules": {
    "max-len": ["error", { "code": 120, "ignoreStrings": true }],
    "spaced-comment": "off",
    "no-alert": "off",
  },
  "env": {
    "browser": true,
    "node": true
  }
}
